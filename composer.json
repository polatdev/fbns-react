{
  "name": "polatdev/fbns-react",
  "description": "A PHP client for the FBNS built on top of ReactPHP",
  "keywords": [
    "FBNS",
    "Client",
    "PHP"
  ],
  "type": "library",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Abdulkadir Polat",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Fbns\\Client\\": "src/"
    }
  },
  "require": {
    "php": "^8.0",
    "ext-mbstring": "*",
    "ext-zlib": "*",
    "evenement/evenement": "~2.0|~3.0",
    "react/event-loop": "0.4.3",
    "react/promise": "^2.7",
    "react/socket": "^1.3",
    "polatdev/net-mqtt": "^1.0",
    "psr/log": "^3.0"
  },
  "suggest": {
    "ext-event": "For more efficient event loop implementation.",
    "ext-gmp": "To be able to run this code on x86 PHP builds."
  }
}
